	public void tearDown() throws Exception {
	}

	@Test
	public void testReverse() {
		l.add(1);
		l.add(2);
		l.add(3);
		l.add(4);
		l.reverse();
		for(int i=0;i<l.size();i++){
			System.out.println(l.get(i));
		}
	}

